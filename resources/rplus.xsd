<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--Please add namespace attributes, a targetNamespace attribute and import elements according to your requirements-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.epo.org/register" targetNamespace="http://www.epo.org/register" elementFormDefault="qualified" version="20161030">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace"/>
	<xs:element name="register-documents">
		<xs:complexType>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="register-document"/>
			</xs:sequence>
			<xs:attribute name="date-produced" type="xs:anySimpleType"/>
			<xs:attribute name="dtd-version" type="xs:anySimpleType"/>
			<xs:attribute name="produced-by" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="DO"/>
						<xs:enumeration value="ISA"/>
						<xs:enumeration value="EO"/>
						<xs:enumeration value="RO"/>
						<xs:enumeration value="applicant"/>
						<xs:enumeration value="IB"/>
						<xs:enumeration value="IPEA"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ro" type="xs:anySimpleType"/>
			<xs:attribute name="status" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="register-document">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ep-patent-statuses"/>
				<xs:element ref="bibliographic-data"/>
				<xs:element ref="procedural-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="events-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="file-wrapper-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="unitary-patent" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="date-produced" type="xs:anySimpleType"/>
			<xs:attribute name="dtd-version" type="xs:anySimpleType"/>
			<xs:attribute name="file" type="xs:anySimpleType"/>
			<xs:attribute name="lang" type="xs:anySimpleType"/>
			<xs:attribute name="produced-by" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="DO"/>
						<xs:enumeration value="ISA"/>
						<xs:enumeration value="EO"/>
						<xs:enumeration value="RO"/>
						<xs:enumeration value="applicant"/>
						<xs:enumeration value="IB"/>
						<xs:enumeration value="IPEA"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ro" type="xs:anySimpleType"/>
			<xs:attribute name="status" type="xs:anySimpleType"/>
			<xs:attribute name="file-reference-id" type="xs:anySimpleType"/>
			<xs:attribute name="unitary-patent-status" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ep-patent-statuses">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ep-patent-status" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ep-patent-status">
		<xs:complexType mixed="true">
			<xs:attribute name="change-date" type="xs:anySimpleType" use="required"/>
			<xs:attribute name="status-code" type="xs:anySimpleType" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="bibliographic-data">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="plain-language-designation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="publication-reference" maxOccurs="unbounded"/>
				<xs:element ref="classification-ipc" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="classifications-ipcr" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="classification-national" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="classification-locarno" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="classification-udc" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="application-reference" maxOccurs="unbounded"/>
				<xs:element ref="language-of-filing" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="language-of-publication" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="priority-claims" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="parties"/>
				<xs:element ref="designation-of-states" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="invention-title" maxOccurs="unbounded"/>
				<xs:element ref="assignees" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="licensee-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-of-earliest-priority" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-complete-specification-filed" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="dates-of-public-availability" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="term-of-grant" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="dates-rights-effective" minOccurs="0"/>
				<xs:element ref="patent-family" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="field-of-search" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="figures" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="references-cited" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="examiners" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="number-of-claims" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="related-documents" minOccurs="0"/>
				<xs:element ref="st50-republication" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-exhibition-filed" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-pct-article-22-39-fulfilled" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="pct-application-no-longer-effective" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="bio-deposit" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="pct-or-regional-filing-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="pct-or-regional-publishing-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="defer-publication-of-search-report" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-application-refused" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-rights-reestablished" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-application-withdrawn-by-applicant" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-application-deemed-withdrawn" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-of-revocation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="cmea-havana-agreement" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="date-application-partially-withdrawn" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="office-specific-bib-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="opposition-data" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="text" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="search-reports-information" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="country" type="xs:anySimpleType"/>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="lang" type="xs:anySimpleType"/>
			<xs:attribute name="status" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="plain-language-designation">
		<xs:complexType mixed="true">
			<xs:attribute name="lang" type="xs:anySimpleType"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="publication-reference">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="document-id">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element ref="doc-number"/>
				<xs:element ref="kind" minOccurs="0"/>
				<xs:element ref="name" minOccurs="0"/>
				<xs:element ref="date" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="lang" type="xs:anySimpleType"/>
			<xs:attribute name="document-id-type" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="country">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="doc-number">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="kind">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="name">
		<xs:complexType mixed="true">
			<xs:attribute name="name-type">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="natural"/>
						<xs:enumeration value="legal"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="name-format">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="as-filed"/>
						<xs:enumeration value="standardized"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="date">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="classification-ipc">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="edition"/>
				<xs:element ref="main-classification"/>
				<xs:element ref="further-classification" minOccurs="0" maxOccurs="unbounded"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="additional-info"/>
					<xs:element ref="linked-indexing-code-group"/>
					<xs:element ref="unlinked-indexing-code"/>
				</xs:choice>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="main-classification">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="further-classification">
		<xs:complexType mixed="true">
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="sequence" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="text">
		<xs:complexType mixed="true">
			<xs:attribute name="lang" type="xs:anySimpleType"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="edition">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="additional-info">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="p" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="p">
		<xs:complexType mixed="true">
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="num" type="xs:anySimpleType" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="linked-indexing-code-group">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="main-linked-indexing-code"/>
				<xs:element ref="sub-linked-indexing-code" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="main-linked-indexing-code">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="sub-linked-indexing-code">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="unlinked-indexing-code">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="classifications-ipcr">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="classification-ipcr" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-ipcr">
		<xs:complexType>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="ipc-version-indicator"/>
					<xs:element ref="classification-level" minOccurs="0"/>
					<xs:element ref="section"/>
					<xs:element ref="class"/>
					<xs:element ref="subclass"/>
					<xs:element ref="main-group" minOccurs="0"/>
					<xs:element ref="subgroup" minOccurs="0"/>
					<xs:element ref="symbol-position" minOccurs="0"/>
					<xs:element ref="classification-value" minOccurs="0"/>
					<xs:element ref="action-date" minOccurs="0"/>
					<xs:element ref="generating-office" minOccurs="0"/>
					<xs:element ref="classification-status" minOccurs="0"/>
					<xs:element ref="classification-data-source" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="text"/>
			</xs:choice>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="sequence" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="section">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="class">
		<xs:complexType mixed="true">
			<xs:attribute name="class-type" type="xs:anySimpleType"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="subclass">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="main-group">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="subgroup">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="ipc-version-indicator">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-level">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="symbol-position">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="classification-value">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="action-date">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-status">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="classification-data-source">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="generating-office">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-national">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element ref="edition" minOccurs="0"/>
				<xs:element ref="main-classification"/>
				<xs:element ref="further-classification" minOccurs="0" maxOccurs="unbounded"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="additional-info"/>
					<xs:element ref="linked-indexing-code-group"/>
					<xs:element ref="unlinked-indexing-code"/>
				</xs:choice>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-locarno">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="edition"/>
				<xs:element ref="main-classification"/>
				<xs:element ref="further-classification" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="text" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="classification-udc">
		<xs:complexType mixed="true">
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="application-reference">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="document-id" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="appl-type" type="xs:anySimpleType"/>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="language-of-filing">
		<xs:complexType mixed="true">
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="language-of-publication">
		<xs:complexType mixed="true">
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-claims">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="priority-claim" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="incorporation-by-reference" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-claim">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
				<xs:element ref="doc-number" minOccurs="0"/>
				<xs:element ref="date"/>
				<xs:element ref="office-of-filing" minOccurs="0"/>
				<xs:choice minOccurs="0">
					<xs:element ref="priority-doc-attached"/>
					<xs:element ref="priority-doc-requested"/>
				</xs:choice>
				<xs:element ref="restore-rights" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="kind" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="international"/>
						<xs:enumeration value="regional"/>
						<xs:enumeration value="national"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="sequence" type="xs:anySimpleType" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="office-of-filing">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="country"/>
				<xs:sequence>
					<xs:element ref="region"/>
					<xs:element ref="country" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="region">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="country"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="priority-doc-requested">
		<xs:complexType/>
	</xs:element>
	<xs:element name="priority-doc-attached">
		<xs:complexType/>
	</xs:element>
	<xs:element name="restore-rights">
		<xs:complexType/>
	</xs:element>
	<xs:element name="incorporation-by-reference">
		<xs:complexType/>
	</xs:element>
	<xs:element name="parties">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="applicants" maxOccurs="unbounded"/>
				<xs:element ref="inventors" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="correspondence-address" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="agents" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="applicants">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="applicant" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="transfer-of-rights">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="yes"/>
						<xs:enumeration value="no"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="change-date" type="xs:anySimpleType"/>
			<xs:attribute name="change-gazette-num" type="xs:anySimpleType"/>
			<xs:attribute name="value-valid-for-publications" type="xs:IDREFS"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="applicant">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="addressbook" maxOccurs="unbounded"/>
				<xs:element ref="nationality"/>
				<xs:element ref="residence"/>
